{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "watchPath": "C:\\ImageDrop",
  "ImageConfig": {
    "compressionLevel": 0.7,
    "outputPath": "Y:\\ImageOutput\\Dev",
    "thumbnail": {
      "width": 48,
      "filePrefix": "thumbnail-"
    },
    "medium": {
      "width": 300,
      "filePrefix": "medium-"
    },
    "large": {
      "width": 720,
      "filePrefix": "large-",
      "watermarkText": "Carved Rock Fitness"
    }
  }
}
